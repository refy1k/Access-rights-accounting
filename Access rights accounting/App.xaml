<Application x:Class="Access_rights_accounting.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Access_rights_accounting"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="IntroText">
            <Setter Property="TextBlock.FontSize" Value="90"/>
            <Setter Property="TextBlock.Foreground" Value="White"/>
            <Setter Property="TextBlock.FontFamily" Value="Franklin Gothic Demi Cond"/>
        </Style>
    </Application.Resources>
</Application>
